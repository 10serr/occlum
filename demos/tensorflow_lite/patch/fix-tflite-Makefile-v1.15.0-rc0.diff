+LDOPTS := -L/usr/local/occlum/x86_64-linux-musl/lib
+LDFLAGS := -pie $(LDOPTS)
 ARFLAGS := -r
 TARGET_TOOLCHAIN_PREFIX :=
 CC_PREFIX :=
@@ -80,6 +81,12 @@ BENCHMARK_BINARY_NAME := benchmark_model
 MINIMAL_SRCS := \
 	tensorflow/lite/examples/minimal/minimal.cc
 
